#include<bits/stdc++.h>
int main()
{
    std::vector<int> vect1{1,3,2,4,3,4,7,6,5,6,7,8};
    
    //find function
    int foundSearch = 7;
    int notFindSearch = 76;
    
    std::vector<int>::iterator it;
    
    it = std::find(vect1.begin(),vect1.end(),foundSearch);
    if(it == vect1.end())
    {
        std::cout<<" Not found 7";
    }
    
    it = std::find(vect1.begin(),vect1.end(),notFindSearch);
    if(it == vect1.end())
    {
        std::cout<<" not found 76";
    }
    
    //count 
    
    int count = 0;
    count = std::count(vect1.begin(),vect1.end(),4);
    std::cout<<"\ncount of 4 = "<<count;
    
    //Sort 
    std::cout<<"\n";
    for(auto elem:vect1)
        std::cout<<" "<<elem;
    sort(vect1.begin(),vect1.end());
    std::cout<<"\n";
    for(auto elem:vect1)
        std::cout<<" "<<elem;
    return 0;
}


output:
 not found 76                                                                                                         
count of 4 = 2                                                                                                        
 1 3 2 4 3 4 7 6 5 6 7 8                                                                                              
 1 2 3 3 4 4 5 6 6 7 7 8
